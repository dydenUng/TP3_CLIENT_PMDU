<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="searchPane" spacing="2.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controleur.RecherchePaneCtrl">
   <children>
      <Label fx:id="lblTitleSearch" minHeight="0.0" text="Ma recherche" />
      <HBox spacing="10.0">
         <children>
            <TextField fx:id="champSearch" minHeight="0.0" />
            <Button fx:id="btnSearch" minHeight="0.0" mnemonicParsing="false" text="Chercher" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="7.0" left="7.0" right="7.0" top="7.0" />
   </padding>
</VBox>
